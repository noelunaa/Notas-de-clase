CREATE TABLE covid (id INT(6) UNSIGNED AUTO_INCREMENT PRIMARY KEY, fecha TIMESTAMP DEFAULT CURRENT_TIMESTAMP, nombre CHAR (248), temp FLOAT(4,2) NOT NULL, sp02 INT(1) UNSIGNED NOT NULL, bpm INT(1) UNSIGNED NOT NULL);

CREATE TABLE covid (
	id INT(6) UNSIGNED AUTO_INCREMENT PRIMARY KEY, 
	fecha TIMESTAMP DEFAULT CURRENT_TIMESTAMP, 
	nombre CHAR (248), 
	temp FLOAT(4,2) NOT NULL, 
	sp02 INT(1) UNSIGNED NOT NULL, 
	bpm INT(1) UNSIGNED NOT NULL);
